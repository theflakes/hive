{
	"reverse_dns": true,
	"l2_attribution": false,
	"field_names": {
		"data_type": {
			"name": "data_type",
			"default": "Honey"
		},
		"default_gateway": {
			"name": "default_gateway",
			"default": null
		},
		"device_name": {
			"name": "device_name",
			"default": null
		},
		"dst_ip": {
			"name": "dst_ip",
			"default": null
		},
		"dst_port": {
			"name": "dst_port",
			"default": null
		},
		"hostname": {
			"name": "hostname",
			"default": null
		},
		"interface_name": {
			"name": "interface_name",
			"default": null
		},
		"ip_proto": {
			"name": "ip_proto",
			"default": null
		},
		"message": {
			"name": "message",
			"default": null
		},
		"src_ip": {
			"name": "src_ip",
			"default": null
		},
		"src_mac": {
			"name": "src_mac",
			"default": null
		},
		"src_port": {
			"name": "src_port",
			"default": null
		},
		"subnet_mask": {
			"name": "subnet_mask",
			"default": null
		},
		"src_vendor": {
			"name": "src_vendor",
			"default": null
		},
		"timestamp": {
			"name": "timestamp",
			"default": null
		}
	},
	"ll_mcast": [
		{
			"protocol": "tcp",
			"ports": []
		},
		{
			"protocol": "udp",
			"ports": [137, 138]
		}
	],
	"timeout": 1,
	"honey": [
		{
			"config": "./config/listeners/smb.json",
			"logger": "smb"
		},
		{
			"config": "./config/listeners/rdp.json",
			"logger": "rdp"
		},
		{
			"config": "./config/listeners/wsman.json",
			"logger": "wsman"
		}
	],
	"default_logger": "root",
	"logging": {
		"version": 1,
		"formatters": {
			"standard": {
				"format": "%(message)s"
			}
		},
		"handlers": {
			"error": {
				"level": "ERROR",
				"formatter": "standard",
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "error.log",
				"mode": "a",
				"maxBytes": 10485760,
				"backupCount": 5
			},
			"basic": {
				"level": "DEBUG",
				"formatter": "standard",
				"class": "logging.StreamHandler"
			},
			"smb": {
				"level": "INFO",
				"formatter": "standard",
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "./logs/smb.log",
				"mode": "a",
				"maxBytes": 10485760,
				"backupCount": 5
			},
			"rdp": {
				"level": "INFO",
				"formatter": "standard",
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "./logs/rdp.log",
				"mode": "a",
				"maxBytes": 10485760,
				"backupCount": 5
			},
			"wsman": {
				"level": "INFO",
				"formatter": "standard",
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "./logs/wsman.log",
				"mode": "a",
				"maxBytes": 10485760,
				"backupCount": 5
			}
		},
		"loggers": {
			"root": {
				"handlers": ["basic"],
				"level": "INFO",
				"propagate": true
			},
			"smb": {
				"handlers": ["smb"],
				"level": "INFO",
				"propagate": true
			},
			"rdp": {
				"handlers": ["rdp"],
				"level": "INFO",
				"propagate": true
			},
			"wsman": {
				"handlers": ["wsman"],
				"level": "INFO",
				"propagate": true
			}
		}
	}
}